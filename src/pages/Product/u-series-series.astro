---
import Layout from '../../layouts/Layout.astro';

// Product data
const product = {
  name: "U Series LED Display",
  tagline: "Ultra-Fine Pixel Pitch LED Panel in Dubai",
  description: "The U Series from Lovosis Technology represents the pinnacle of LED display precision with revolutionary micro-LED architecture. Featuring ultra-fine pixel pitch options as small as P0.9, the U Series delivers unprecedented visual clarity and color accuracy for premium installations across UAE. Engineered specifically for Dubai's high-end corporate environments, luxury retail displays, and mission-critical control rooms, these advanced LED panels offer exceptional image quality with perfect uniformity, superior contrast ratios, and seamless display integration.",
  image: "/LED Displays/img4.webp",
  features: [
    "Ultra-fine pixel pitch (P0.9, P1.2, P1.5)",
    "Exceptional brightness uniformity (≥98%)",
    "Superior contrast ratio (8000:1)",
    "HDR10+ certified visual performance",
    "Wide color gamut (145% sRGB)",
    "Advanced calibration technology",
    "Zero moiré effect for camera recording",
    "Low heat emission for Dubai environments",
    "Low power consumption design",
    "UAE dust protection technology"
  ],
  specifications: [
    { name: "Pixel Pitch", value: "P0.9 / P1.2 / P1.5 options" },
    { name: "Panel Resolution", value: "256×256 pixels per cabinet" },
    { name: "Panel Size", value: "500mm × 500mm" },
    { name: "Brightness", value: "800 nits" },
    { name: "Refresh Rate", value: "≥7680Hz" },
    { name: "Viewing Angle", value: "170° wide angle" },
    { name: "IP Rating", value: "IP50 (front) / IP30 (rear)" },
    { name: "Lifespan", value: "100,000+ hours" },
    { name: "Working Temperature", value: "-10°C ~ 45°C (optimal for UAE)" },
    { name: "Control System", value: "NovaStar / Brompton / Colorlight" }
  ],
  applications: [
    "Luxury Retail in Dubai Mall",
    "UAE Executive Boardrooms",
    "Dubai Command & Control Centers",
    "UAE Broadcast Studios",
    "High-end Museums in Dubai",
    "Flagship Brand Stores UAE",
    "Financial Trading Floors Dubai",
    "Premium Hospitality in UAE"
  ]
};

// Related products
const relatedProducts = [
  {
    name: "XEB Series",
    description: "Premium indoor LED display solution with fine pixel pitch technology for corporate and retail environments across Dubai and UAE",
    image: "/LED Displays/XEB Series.png",
    slug: "xeb-series-series",
    features: ["Fine pixel pitch", "High brightness", "Seamless connection", "Premium quality"]
  }
];

// SEO-optimized title, description and keywords with Dubai/UAE focus
const title = "U Series Ultra-Fine Pixel LED Panel Dubai | P0.9 LED Display UAE | Premium LED Wall | Lovosis"
const description = "U Series Ultra-Fine Pixel LED Panel in Dubai featuring micro-LED P0.9 technology. Premium LED displays in UAE with superior image quality. Professional LED video walls for luxury retail and corporate environments across Dubai."
const keywords = "U Series LED Panel Dubai, P0.9 LED display UAE, ultra-fine pixel LED Dubai, micro-LED technology UAE, premium LED video wall Dubai, luxury retail displays UAE, high-resolution LED screen Dubai"

// Enhanced schema markup for SEO
const productSchema = {
  "@context": "https://schema.org/",
  "@type": "Product",
  "name": "U Series Ultra-Fine Pixel LED Panel in Dubai",
  "image": `https://lovosisdisplay.ae${product.image}`,
  "description": "U Series Ultra-Fine Pixel LED Panel in Dubai featuring micro-LED P0.9 technology. Premium LED displays in UAE with superior image quality. Professional LED video walls for luxury retail and corporate environments.",
  "brand": {
    "@type": "Brand",
    "name": "Lovosis Display Dubai"
  },
  "offers": {
    "@type": "Offer",
    "url": "https://lovosisdisplay.ae/Product/u-series-series",
    "priceCurrency": "AED",
    "priceValidUntil": "2026-06-01",
    "availability": "https://schema.org/InStock",
    "areaServed": {
      "@type": "GeoCircle",
      "geoMidpoint": {
        "@type": "GeoCoordinates",
        "latitude": "25.276987",
        "longitude": "55.296249"
      },
      "geoRadius": "200"
    },
    "seller": {
      "@type": "Organization",
      "name": "Lovosis Display Dubai - Premium LED Panel UAE Specialist"
    }
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "5.0",
    "reviewCount": "37",
    "bestRating": "5",
    "worstRating": "1"
  },
  "review": {
    "@type": "Review",
    "reviewRating": {
      "@type": "Rating",
      "ratingValue": "5",
      "bestRating": "5"
    },
    "author": {
      "@type": "Person",
      "name": "Ahmed Al Falasi"
    },
    "datePublished": "2025-03-10",
    "reviewBody": "We installed the U Series LED Panel with P0.9 pixel pitch in our Dubai luxury retail showroom, and the results are spectacular. The ultra-fine pixel technology delivers incredibly crisp visuals that showcase our products with perfect clarity and color reproduction. Lovosis provided exceptional service from consultation to installation."
  }
};

// FAQ data for schema and content
const faqs = [
  {
    question: "What makes U Series P0.9 LED panels special for luxury retail in Dubai?",
    answer: "U Series P0.9 LED panels feature ultra-fine pixel pitch technology that delivers exceptional clarity even at close viewing distances, making them ideal for Dubai's luxury retail environments where product detail visualization is critical. The superior color accuracy and contrast ratio ensure premium brand presentations that meet the high standards of UAE luxury establishments."
  },
  {
    question: "How do U Series LED panels perform in Dubai's climate conditions?",
    answer: "U Series LED panels are specifically engineered for UAE's climate with advanced thermal management systems and specialized dust protection. The panels maintain optimal performance in Dubai's indoor environments with consistent visual quality even during extended operating hours in air-conditioned spaces."
  },
  {
    question: "What is the resolution capability of U Series LED panels in UAE installations?",
    answer: "U Series LED panels in Dubai installations can achieve exceptional resolutions up to 8K depending on the wall size, with the P0.9 pixel pitch providing the highest pixel density. This makes them ideal for UAE applications requiring precise detail reproduction such as control rooms, luxury retail, and corporate showcases."
  },
  {
    question: "Are U Series LED panels compatible with existing control systems in Dubai?",
    answer: "Yes, U Series LED panels integrate seamlessly with all major control systems used across UAE, including NovaStar, Brompton, and Colorlight processors. This ensures compatibility with existing AV infrastructure in Dubai installations while delivering superior visual performance."
  },
  {
    question: "What maintenance support is provided for U Series LED panels in UAE?",
    answer: "All U Series LED panels installed in UAE come with comprehensive local support including 24/7 technical assistance, regular maintenance visits, and a 5-year warranty. Our Dubai-based team provides prompt service to ensure your premium LED display continues to perform at optimal levels throughout its lifespan."
  }
];
---

<Layout title={title} description={description} keywords={keywords} sd={productSchema}>
  <div class="bg-white">
    <!-- Breadcrumb -->
    <div class="bg-gray-100 py-4">
      <div class="container mx-auto px-4">
        <div class="flex items-center text-sm text-gray-600">
          <a href="/" class="hover:text-blue-600">Home</a>
          <svg class="w-3 h-3 mx-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
          </svg>
          <a href="/Products/led-display" class="hover:text-blue-600">LED Displays</a>
          <svg class="w-3 h-3 mx-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
          </svg>
          <span class="font-medium text-gray-900">U Series Ultra-Fine Pixel LED Panel Dubai</span>
        </div>
      </div>
    </div>

    <!-- Product Hero Section -->
    <section class="py-16">
      <div class="container mx-auto px-4">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-12">
          <!-- Product Image -->
          <div class="bg-white rounded-xl shadow-lg overflow-hidden p-6">
            <img 
              src={product.image} 
              alt="U Series Ultra-Fine Pixel LED Panel in Dubai - P0.9 LED Display UAE"
              class="w-full h-auto object-contain"
            />
            
            <!-- Product Badge -->
            <div class="mt-6 inline-flex items-center px-4 py-2 bg-purple-100 text-purple-800 rounded-full text-sm font-medium">
              Ultra Premium P0.9 LED Panel in Dubai
            </div>
            
            <!-- Product Review Summary -->
            <div class="mt-8 border-t pt-6">
              <div class="flex items-center justify-between">
                <h3 class="text-lg font-semibold text-gray-900">Customer Rating</h3>
                <div class="flex items-center gap-1">
                  <div class="flex">
                    {[1, 2, 3, 4, 5].map((star) => (
                      <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
